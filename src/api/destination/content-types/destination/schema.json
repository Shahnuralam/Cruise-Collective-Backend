{
  "kind": "collectionType",
  "collectionName": "destinations",
  "info": {
    "singularName": "destination",
    "pluralName": "destinations",
    "displayName": "Destination",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "excerpt": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "continent",
        "country",
        "place"
      ]
    },
    "offer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::offer.offer",
      "inversedBy": "destinations"
    },
    "destination": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::destination.destination"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
